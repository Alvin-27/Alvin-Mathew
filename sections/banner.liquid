{% for blocks in section %}
  {% if blocks.id== 'Heading' %}
    {{ blocks.settings.Heading }}
  {% endif %}
  
{% endfor %}
{% schema %}
{
  "name" : "Banner",
  "settings": [
  ],
  "presets" : [
    {
      "name" : "default banner settings"
    }
  ],
  "blocks": [
    {
      "type": "text_block",
      "name" : "Header",
      "settings": [
        {
          "type": "text",
          "id": "Heading",
          "label" : "Heading",
          "default" : "Tisso Vision"
        }
      ]
  

    }
  ]
}
{% endschema %}